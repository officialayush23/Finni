{
  "expo": {
    "name": "Finni",
    "slug": "expo-router-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.exporouterexample",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "Allow Finni to use speech recognition for financial commands.",
        "NSMicrophoneUsageDescription": "Allow Finni to use the microphone to listen to your queries."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.exporouterexample"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow Finni to access the mic.",
          "speechRecognitionPermission": "Allow Finni to process your voice."
        }
      ],
      "@react-native-voice/voice"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d086680f-0546-44ba-ab89-7fef9563009b"
      }
    }
  }
}
